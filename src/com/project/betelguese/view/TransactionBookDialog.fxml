<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" styleClass="background" stylesheets="@../assets/css/add-book-screen-model.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.betelguese.app.screens.TransactionBookScreen">
   <children>
      <VBox layoutX="51.0" layoutY="14.0" spacing="20.0">
         <children>
            <TextField fx:id="bookIdField" promptText="BooksId" />
            <TextField fx:id="amountField" promptText="Amount" />
            <HBox spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleOkButton" text="ok">
                     <font>
                        <Font name="MuseoSansRounded-700" size="12.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleCancelButton" text="cancel">
                     <font>
                        <Font name="MuseoSansRounded-700" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
